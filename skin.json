{
	"name": "PoeWiki",
	"version": "0.1",
	"author": "Alx, MiÅ‚osz Biedrzycki",
	"url": "https://github.com/alx-pl/PoeWiki",
	"descriptionmsg": "poewiki-desc",
	"namemsg": "skinname-poewiki",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"ValidSkinNames": {
		"poewiki": "PoeWiki"
	},
	"MessagesDirs": {
		"PoeWiki": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.poewiki": {
			"class": "ResourceLoaderSkinModule",
			"features": [
				"normalize",
				"elements",
				"content",
				"interface",
				"logo"
			],
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.less": {
					"media": "screen and (min-width: 751px)"
				},
				"resources/screen-mobile.less": {
					"media": "screen and (max-width: 750px)"
				},
				"resources/print.css": {
					"media": "print"
				}
			}
		},
		"skins.poewiki.js": {
			"scripts": [
				"resources/main.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "PoeWiki"
	},
	"ResourceModuleSkinStyles": {
		"poewiki": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"Hooks": {
		"ArticleEditUpdatesDeleteFromRecentchanges": "SkinPoeWiki::onArticleEditUpdatesDeleteFromRecentchanges"
	},
	"AutoloadClasses": {
		"SkinPoeWiki": "includes/SkinPoeWiki.php",
		"PoeWikiTemplate": "includes/PoeWikiTemplate.php",
	},
	"manifest_version": 1
}
